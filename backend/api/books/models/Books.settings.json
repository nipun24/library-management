{
  "connection": "default",
  "collectionName": "books",
  "info": {
    "name": "books",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "isbn": {
      "type": "biginteger",
      "required": true,
      "unique": true
    },
    "copies": {
      "type": "integer",
      "required": true,
      "default": "1"
    },
    "issued": {
      "type": "integer",
      "default": "0",
      "required": true
    },
    "author": {
      "type": "string",
      "required": true
    }
  }
}